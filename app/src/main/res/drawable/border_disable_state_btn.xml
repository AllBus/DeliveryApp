<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item
		android:bottom="2dp"
		android:left="2dp"
		android:right="2dp"
		android:top="2dp">
		<shape xmlns:android="http://schemas.android.com/apk/res/android"
			   android:shape="rectangle">
			<padding
				android:bottom="2dp"
				android:left="2dp"
				android:right="2dp"
				android:top="2dp"/>
			<corners
				android:radius="4dp"/>
			<solid
				android:color="@color/borderDisableFill"/>
			<stroke
				android:width="1dp"
				android:color="@color/borderDisableBorder"/>
		</shape>
	</item>
</layer-list>
